{
  "$schema": "https://cli.sasjs.io/sasjsconfig-schema.json",
  "macroFolders": ["sasjs/macros"],
  "serviceConfig": {
    "serviceFolders": ["sasjs/services/common"]
  },
  "defaultTarget": "viya",
  "targets": [
    {
      "name": "viya",
      "serverType": "SASVIYA",
      "serverUrl": "https://sas.analytium.co.uk",
      "appLoc": "/Public/app/fileuploader",
      "deployConfig": {
        "deployServicePack": true,
        "deployScripts": ["sasjs/build/copyscript.sh"]
      },
      "streamConfig": {
        "assetPaths": [],
        "streamWeb": true,
        "streamWebFolder": "webv",
        "webSourcePath": "src"
      }
    }
  ]
}
